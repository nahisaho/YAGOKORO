{
  "name": "@yagokoro/normalizer",
  "version": "2.0.0",
  "private": true,
  "description": "Entity normalization pipeline for YAGOKORO GraphRAG system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@yagokoro/domain": "workspace:*",
    "@yagokoro/neo4j": "workspace:*",
    "@yagokoro/vector": "workspace:*",
    "fastest-levenshtein": "^1.0.16",
    "compromise": "^14.14.3",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "vitest": "^2.1.8"
  },
  "keywords": ["normalization", "entity", "alias", "graphrag"]
}
