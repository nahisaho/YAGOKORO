{
  "name": "yagokoro",
  "version": "0.4.0",
  "private": true,
  "description": "GraphRAG MCP System for AGI - Generative AI Genealogy Knowledge System",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:unit": "pnpm -r test:unit",
    "test:integration": "pnpm -r test:integration",
    "test:e2e": "pnpm -r test:e2e",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "dev": "pnpm -r --parallel dev",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:dev": "docker compose -f docker/docker-compose.dev.yml up -d",
    "prepare": "echo 'Ready to develop!'"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^20.17.10",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": ["graphrag", "knowledge-graph", "mcp", "agi", "generative-ai"],
  "author": "YAGOKORO Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/yagokoro.git"
  }
}
