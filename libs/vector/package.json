{
  "name": "@yagokoro/vector",
  "version": "0.1.0",
  "private": true,
  "description": "Qdrant vector store implementation for YAGOKORO GraphRAG system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run --exclude='**/*.integration.test.ts'",
    "test:integration": "vitest run --include='**/*.integration.test.ts'",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@yagokoro/domain": "workspace:*",
    "@qdrant/js-client-rest": "^1.12.0"
  },
  "devDependencies": {
    "testcontainers": "^10.18.0",
    "vitest": "^2.1.8"
  },
  "keywords": ["qdrant", "vector-store", "embeddings", "similarity-search"]
}
