{
  "name": "@yagokoro/domain",
  "version": "0.1.0",
  "private": true,
  "description": "Domain models and ports for YAGOKORO GraphRAG system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "devDependencies": {
    "vitest": "^2.1.8"
  },
  "keywords": ["domain", "entities", "ports", "graphrag"]
}
