{
  "name": "@yagokoro/temporal",
  "version": "4.0.0",
  "description": "Time-series analysis for YAGOKORO - trend detection, adoption phases, and temporal queries",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
